    "#Read other words\n",
    "other_words_file = open(\"../rawdata/other_words.txt\")\n",
    "raw_other_words = other_words_file.read()\n",
    "other_words = raw_other_words.splitlines()\n",
    "#print(other_words[:10])\n",
    "\n",
    "#combine them\n",
    "words = sorted(answers + other_words)"
