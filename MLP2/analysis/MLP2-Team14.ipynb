    "cur_id = 0\n",
    "\n",
    "#process each tweet to make text machine readable\n",
    "for index, tweet in wordleTweets.iterrows():\n",
    "    \n",
    "    #Set first index\n",
    "    if index == 0:\n",
    "        cur_id = tweet.wordle_id\n",
    "        \n",
    "    #find all guess emoju strings\n",
    "    raw_guesses = re.findall(\"([游릴游릳拘拘]+)\", tweet.tweet_text)\n",
    "    guesses = []\n",
    "    \n",
    "    #replace emojs with letter\n",
    "    for guess in raw_guesses:\n",
    "        guess = guess.replace(\"游릴\", 'G')\n",
    "        guess = guess.replace(\"游릳\", 'Y')\n",
    "        guess = guess.replace(\"拘뜦", 'B')\n",
    "        guess = guess.replace(\"拘\", 'B')\n",
    "        guesses.append(guess)\n",
    "        \n",
    "    single_game = tiles_to_characters(tweet.tweet_text)\n",
